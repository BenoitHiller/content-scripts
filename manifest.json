{
  "manifest_version":2,
  "name":"Benoit's Content Scripts",
  "description":"Content scripts for various pages",
  "version":"1.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["http://www.octopuspie.com/*"],
      "js": ["hotkeys.js", "octopuspie.js"]
    },
    {
      "matches": ["http://www.awkwardzombie.com/*"],
      "js": ["hotkeys.js", "awkwardzombie.js"]
    },
    {
      "matches": ["http://gunshowcomic.com/*"],
      "js": ["hotkeys.js", "gunshow.js"]
    },
    {
      "matches": ["http://www.buzzfeed.com/*"],
      "js": ["jquery-2.1.3.min.js","buzzfeed.js"]
    },
    {
      "matches": ["http://www.google.com/analytics/"],
      "run_at": "document_start",
      "js": ["analytics.js"]
    },
    {
      "matches": ["https://www.irccloud.com/*"],
      "css": ["irccloud.css"]
    },
    {
      "matches": ["https://developer.mozilla.org/*"],
      "css": ["mdn.css"]
    },
    {
      "matches": ["https://cloud.digitalocean.com/droplets/*"],
      "css": ["digital-ocean.css"]
    },
    {
      "matches": ["https://doc.rust-lang.org/*"],
      "css": ["rust-lang.css"],
      "js": ["jquery-2.1.3.min.js","rust-lang.js"]
    }
  ]
}
